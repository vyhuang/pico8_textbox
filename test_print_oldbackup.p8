function replace_all(str)
 local str_n="\014"
 local char
 local char_num
 local replace_s
 for i=1,#str do
  char=sub(str,i,i)
  replace_s=char_table[ord(char)]
  if ord(char) < 128 then
   if replace_s then
   	str_n=str_n..char..replace_s
   else
 		 str_n=str_n..char.."\-e"
 	 end
 	end
 end
 return str_n
end
